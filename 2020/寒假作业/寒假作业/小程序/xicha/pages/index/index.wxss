/*  */

.container{
  /* background: #ccc; */
  height: 100%;
}
.topBig{
  width: 100%; 
}
.topBig image{
  width: 100%;
  display: block;  
  /* background: #ccc; */
}
.expressage{
  position: relative;
  top: -40rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 688rpx;
  height: 370rpx;
  background-color: #fff;
  border-radius: 20rpx;
  box-shadow: 10rpx 10rpx 0rpx 0 rgb(250, 249, 249);

}
.expressage view{
  position: relative;
  display: inline-block;
  width: 50%;
  text-align: center;
  margin-top: 88rpx;
}
.expressage .cat::after{
  content: "";
  position: absolute;
  top: -30rpx;
  right: 0;
  width:2rpx;
  height: 240rpx;
  background-color: #f2f2f2;
}

.expressage view image{
  width: 87rpx; 
  height: 104rpx;
}

.expressage view h2{
  display: block;
  font-size: 38rpx;
  font-weight: bold;
  margin-top: 18rpx;
  margin-bottom: 10rpx;
}
.expressage view text{
  font-size: 26rpx;
  font-weight: bold;
}
.classify{
  width: 688rpx;
  display: flex;
  position: relative;
  left: 50%;
  transform: translateX(-50%);
  text-align: center;
}
.classify view{
  flex: 1;
  height: 146rpx;
  border-radius: 20rpx;
  background-color: #EAEBEC;
}
.classify :first-child{
  margin-right: 20rpx;
}
.classify view h2{
  position: relative;
  display: block;
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 20rpx;
  margin-left: 40rpx;
}
.classify :first-child h2::after{
  content:"";
  /* position: absolute; */
  display: inline-block;
  top: 0;
  left: 28rpx;
  width: 40rpx;
  height: 40rpx;
  background-color: red;
  background: url(/images/gift.png) no-repeat;
}
.classify text{
  font-size: 24rpx;
  color: #AAAAAA;
}
.buttom{
  width: 688rpx;
  position: relative;
   left: 50%;
  transform: translateX(-50%);
  text-align: center;
  margin-top: 40rpx;
}
.buttom h2{
  display: block;
  font-size: 30rpx;
  font-weight: bold;

}
.integral {
  display: inline-block;
  width:562rpx;
  text-align: left;
  /* border: 1px solid red; */
}
.member{
  display: inline-block;
}
.integral text{
  font-size: 24rpx;
  color: #7f7f7f;
  margin-top: 20rpx;
}
.member text{
  font-size: 26rpx;
  color: #7f7f7f;
}